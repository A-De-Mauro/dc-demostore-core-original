<div class="amp-dc-card-list
{{#iff cards.length '<' 6}}amp-dc-prod-{{cards.length}}-rows
{{else}}amp-dc-prod-3-rows{{/iff}}
    {{#if heroList}}amp-dc-cards-hero{{/if}}
">
    <div class="amp-dc-card-title">
        {{ header }}
    </div>

    <div class="amp-dc-card-list-wrap">
        {{#each cards}}{{templateChooser this @index}}{{/each}}
    </div>
</div>